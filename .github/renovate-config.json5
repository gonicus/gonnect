{
    $schema: "https://docs.renovatebot.com/renovate-schema.json",
    extends: [
        "config:base",
        "helpers:pinGitHubActionDigests"
    ],
    onboarding: false,
    requireConfig: "ignored",
    platform: "github",
    repositories: [
        "gonicus/gonnect"
    ],
    prConcurrentLimit: 0,
    prHourlyLimit: 5,
    semanticCommits: "enabled",
    enabledManagers: [
        "github-actions",
    ],
    dependencyDashboard: true
}
